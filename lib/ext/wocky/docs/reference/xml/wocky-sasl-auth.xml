<indexterm zone="WOCKY-SASL-AUTH-ERROR--CAPS"><primary sortas="WOCKY_SASL_AUTH_ERROR">WOCKY_SASL_AUTH_ERROR</primary></indexterm><programlisting>#define             WOCKY_SASL_AUTH_ERROR</programlisting>
